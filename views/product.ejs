                    <li class="nav-item">
                        <button type="button" class="btn btn-primary" href="/signup">Logout</button>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <!--product session-->
    <section class="margin-top50 py-5">
        <div class="container-product py-xl-5 py-lg-3">
            <div class="title-section text-center mb-md-5 mb-4">
                <h3 class="w3ls-title mb-3">NOW SALE!</h3>
            </div>
            <div class="row mt-3">
                <div class="col-md-5" style="margin: auto;">
                    <div class="product-demo">
                        <a href="#pro1" class="pro2">
                            <img src="images/9.jpg" alt=" " class="img-fluid" />
                            <h4 class="p-mask" style="text-align: center;">MAC 1 - <span>$500</span></h4>
                        </a>
                    </div>
                </div>
                <div class="col-md-5 mt-md-0 mt-4" style="margin: auto;">

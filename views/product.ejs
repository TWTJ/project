                    </div>
                </div>
            </div>
            <!-- product popup 1 -->
            <div id="pro1" class="pop-overlay">
                <div class="popup">
                    <div id="qrcode1"></div>
                    <script>
                        jQuery('#qrcode1').qrcode({
                            render: "table",
                            width: 200,
                            height: 200,
                            text: "http://203.236.220.33:3000/receipt"
                        });	
                    </script>
                    <span class="fa fa-caret-right ml-1" aria-hidden="true"></span>
                    <a class="close" href="#product">×</a>
                </div>
            </div>
            <!-- //product popup 1 -->
            <!-- product popup 2 -->
            <div id="pro2" class="pop-overlay">
                <div class="popup">
                    <div id="qrcode2"></div>
                    <script>
                        jQuery('#qrcode2').qrcode({
                            render: "table",
                            width: 200,
                            height: 200,
                            text: "http://203.236.220.33:3000/receipt2"
                        });	
                    </script>
                    <a class="close" href="#gallery">×</a>
